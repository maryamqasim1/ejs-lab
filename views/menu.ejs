<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Menu</title>
  </head>

  <body>
    <%- include('./partials/nav') %>
    <h1>Full Menu</h1>
    <ul>
      <% RESTAURANT.menu.forEach(x=> { %>
      <li>
        <h3><%= x.name %> - <%= x.price %>$</h3>
        <p>Rating: <%= x.rating %></p>
        <small><%= x.details %></small>
      </li>
      <% }) %>
    </ul>
  </body>
</html>
